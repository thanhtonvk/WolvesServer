@model IEnumerable<WolvesServer.EF.DoiTac>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>Đối tác</h2>


<p>
    @Html.ActionLink("Thêm mới", "Create", null, new
{
    @class = "btn btn-primary"
})
</p>
<table class="table table-bordered table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenDoiTac)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TrangWeb)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ThongTinKhac)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.TenDoiTac)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TrangWeb)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ThongTinKhac)
    </td>
    <td>
        @Html.ActionLink("Sửa", "Edit", new { id = item.Id }, new { @class = "btn btn-success" })

        @Html.ActionLink("Xóa", "Delete", new { id = item.Id }, new { @class = "btn btn-danger" })
    </td>
</tr>
    }

</table>
